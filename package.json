{
  "name": "alpinejs-booster",
  "version": "1.0.0-alpha",
  "description": "alpinejs-booster provides additional directives, magic functions, and utilities for a more productive development experience",
  "author": {
    "name": "Rameel Burhan",
    "email": "rameel-b@hotmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alpinejs-booster/alpinejs-booster.git"
  },
  "keywords": [
    "alpine.js",
    "alpinejs plugin",
    "alpinejs directive",
    "alpinejs magic",
    "alpinejs helper",
    "alpinejs history",
    "alpinejs routing",
    "alpinejs router",
    "alpinejs hotkey",
    "alpinejs shortcut",
    "alpinejs binding"
  ],
  "main": "dist/alpinejs-booster.js",
  "module": "dist/alpinejs-booster.esm.js",
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "npm run clean && cross-env NODE_ENV=production rollup -c",
    "clean": "rimraf dist coverage",
    "test": "vitest run",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "alpinejs": "^3.13.3"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "cross-env": "^7.0.3",
    "jsdom": "^23.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.5",
    "rollup-plugin-bundle-size": "^1.0.3",
    "terser": "^5.26.0",
    "vitest": "^1.2.0"
  }
